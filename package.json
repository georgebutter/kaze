{
  "type": "module",
  "name": "kaze-ui",
  "version": "0.0.19",
  "description": "React tailwind UI components",
  "module": "dist/kaze.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/kaze.es.js",
      "require": "./dist/kaze.umd.js",
      "default": "./dist/kaze.es.js"
    },
    "./config": {
      "require": "./config.js"
    }
  },
  "files": [
    "dist",
    "config.js",
    "README.md"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "dev": "vite --config vite.config.mjs",
    "test": "eslint",
    "prepublish": "yarn build",
    "bump": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "react",
    "ui",
    "tailwind",
    "components"
  ],
  "author": "George Butter",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.8",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  }
}
